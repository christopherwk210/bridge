{
  "name": "bridge",
  "version": "0.0.0",
  "description": "A handy CloneHero library management tool with a built in Chorus client.",
  "scripts": {
    "ng": "ng",
    "start": "node tools/process-manager.js",
    "build": "ng build --prod --base-href=\"./\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "package.win": "electron-packager ./dist/bridge bridge --arch=ia32,x64 --asar --icon=./src/assets/images/system/icons/win/icon.ico --out=./bin/win --overwrite --platform=win32",
    "package.mac": "electron-packager ./dist/bridge bridge --arch=x64 --asar --icon=./src/assets/images/system/icons/mac/icon.icns --out=./bin/mac --overwrite --platform=darwin",
    "package.linux": "electron-packager ./dist/bridge bridge --arch=x64 --asar --out=./bin/linux --overwrite --platform=linux",
    "setup": "npm "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "jquery": "^3.3.1",
    "request": "^2.88.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "vivus": "^0.4.4",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.3",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "electron": "^4.0.5",
    "electron-packager": "^13.1.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/christopherwk210/bridge"
  },
  "license": "GPL-3.0",
  "author": {
    "email": "christopherwk210@gmail.com",
    "name": "Chris Anselmo"
  }
}
